void greet(String name){
 print("Hello $name! Welcome here.");
}
int sum(int a, int b){
 return a+b;
}
double mul(double a, double b, double c){
 return a\*b\*c;
}
void main(){
 greet("ALi");
 int s = sum(5,6);
 print("Sum of 5 and 6 is $s");
 double m = mul(3.5,6.7,5.2);
 print("Product is $m");
}
